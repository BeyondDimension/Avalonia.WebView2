<Project>
	<Import Project="$(MSBuildThisFileDirectory)Version.props" />

	<!-- üëá ÂÖ®Â±Ä Property ÈÖçÁΩÆ -->
	<PropertyGroup>
		<Deterministic>true</Deterministic>
		<LangVersion>preview</LangVersion>
		<Nullable>enable</Nullable>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
		<UseArtifactsOutput>true</UseArtifactsOutput>
		<SuppressTfmSupportBuildWarnings>true</SuppressTfmSupportBuildWarnings>
		<NoWarn>$(NoWarn);NU1504;NU1507;MSB4011;NU5048;NU5104;RS1041</NoWarn>
		<GenerateAssemblyInfo>true</GenerateAssemblyInfo>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NeutralLanguage>zh-Hans</NeutralLanguage>
		<!-- üëá Ê∫êÁîüÊàêÂô®ÁîüÊàêÁöÑ C# ‰∏¥Êó∂Ê∫êÁ†ÅÊñá‰ª∂Âú®ÁîüÊàêÁªìÊùüÂêéÊòØÂê¶‰∏çÂà†Èô§Êñá‰ª∂ÔºåÁî®‰∫éÁîüÊàêÈîôËØØ‰ª£Á†ÅÊó∂Êü•Êñá‰ª∂ÂÜÖÂÆπÁ∫†Ê≠£ÁîüÊàêÂô®ÔºåÂêØÁî®Êó∂‰ºöÂØºËá¥ VS ÁöÑ Android ÁÉ≠ÈáçËΩΩÂú®ÁîüÊàêÊó∂Âá∫Èîô -->
		<!--<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>-->
		<!-- üëá ‰ΩøÁî®ÁºñËØëÁªëÂÆöÊõø‰ª£ÂèçÂ∞ÑÁªëÂÆöÂÆûÁé∞ -->
		<AvaloniaUseCompiledBindingsByDefault>true</AvaloniaUseCompiledBindingsByDefault>
		<!--<AvaloniaRef>PackageReference</AvaloniaRef>-->
		<AvaloniaRef>ProjectReference</AvaloniaRef>
	</PropertyGroup>

	<!-- üëá TFM != .NET Framework 3.5 -->
	<PropertyGroup Condition="!$(TargetFramework.StartsWith('net35'))">
		<ImplicitUsings>true</ImplicitUsings>
	</PropertyGroup>

	<!-- üëá NuGet ÂåÖ‰ø°ÊÅØ -->
	<PropertyGroup>
		<PackageIconUrl>https://avatars.githubusercontent.com/u/79355691</PackageIconUrl>
		<RepositoryType>git</RepositoryType>
		<Copyright>¬©Ô∏è $(Company). All rights reserved.</Copyright>
		<Authors>$(Company)</Authors>
		<FileVersion>$(Version)</FileVersion>
		<AssemblyVersion>$(Version)</AssemblyVersion>
	</PropertyGroup>

	<!-- üëá CompilerVisibleProperty ÈÖçÁΩÆÊ∫êÁîüÊàêÂô®‰∏≠ MSBuild Property ÂèØËßÅÊÄß -->
	<ItemGroup>
		<CompilerVisibleProperty Include="Deterministic" />
	</ItemGroup>
</Project>